<html>   
<body>   
 <div class="container">   
    <h3>STOCK LIST</h3>   
     
      <div>   
        <%= link_to 'Add user', new_user_path %>   
      </div>   
      <br>   
    <table border="2">   
      <thead>   
      <tr>
        <th>id</th>   
        <th>Name</th>   
        <th>age</th>   
        <th>palce</th>   
        <th>phone</th>   
      </tr>   
      </thead>   
      <tbody>   
      <% @user.each do |user| %>   
          <tr>   
            <td><%= user.name %></td>   
            <td><%= user.age %></td>   
            <td><%= user.palce %></td> 
            <td><%= user.phone %></td>
             <div>   
               <td>   
                <%= link_to 'Show',   user_path(user) %>   
                <%= link_to 'Edit',  edit_user_path(user) %>   
                <%= link_to 'Delete', users_delete_path(user), method: :post  %>
              </div>   
            </td>   
          </tr>   
      <% end %>   
      </tbody>   
    </table>   
    </div>   
  </body>   
</html>     
   
   
     
